{
  "framework": "qunit",
  "test_page": "tests/index.html?hidepassed",
  "disable_watching": true,
  "launchers": {
    "SL_Chrome_Current": {
      "command": "ember sauce:launch -- -b chrome --no-ct -u <url>",
      "protocol": "tap"
    },
    "SL_Firefox_Current": {
      "command": "ember sauce:launch -- -b firefox -v 34 --no-ct -u <url>",
      "protocol": "tap"
    },
    "SL_Safari_Current": {
      "command": "ember sauce:launch -- -b safari -v 8 --no-ct -u <url>",
      "protocol": "tap"
    },
    "SL_Safari_Last": {
      "command": "ember sauce:launch -- -b safari -v 7 --no-ct -u <url>",
      "protocol": "tap"
    },
    "SL_MS_Edge": {
      "command": "ember sauce:launch -- -b 'microsoftedge' --no-ct -u <url>",
      "protocol": "tap"
    },
    "SL_IE_11": {
      "command": "ember sauce:launch -- -b 'internet explorer' -v 11 --no-ct -u <url>",
      "protocol": "tap"
    }
  },
  "launch_in_dev": [
    "PhantomJS"
  ],
  "launch_in_ci": [
    "SL_Chrome_Current",
    "SL_Safari_Current",
    "SL_MS_Edge",
    "SL_IE_11",
    "SL_Firefox_Current"
  ]
}
