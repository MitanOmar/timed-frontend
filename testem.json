{
  "framework": "qunit",
  "test_page": "tests/index.html?hidepassed",
  "disable_watching": true,
  "launch_in_dev": [],
  "launch_in_ci": [
    "PhantomJS",
    "SL_Safari_Current",
    "SL_Chrome_Current",
    "SL_Firefox_Current"
  ],
  "launchers": {
    "SL_Safari_Current": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "safari",
        "-p",
        "Mac 10.10",
        "--attach",
        "--no-connect",
        "--url"
      ],
      "protocol": "browser"
    },
    "SL_Chrome_Current": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "chrome",
        "-p",
        "Linux",
        "--attach",
        "--no-connect",
        "--url"
      ],
      "protocol": "browser"
    },
    "SL_Firefox_Current": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "firefox",
        "-p",
        "Linux",
        "--attach",
        "--no-connect",
        "--url"
      ],
      "protocol": "browser"
    }
  }
}
