{
  "framework": "qunit",
  "test_page": "tests/index.html?hidepassed",
  "disable_watching": true,
  "launchers": {
    "SL_Firefox_Current": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "firefox",
        "-v",
        "current",
        "--vi",
        "public",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_Chrome_Current": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "chrome",
        "-v",
        "current",
        "--vi",
        "public",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_Safari_Current": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "safari",
        "-v",
        "9.0",
        "--vi",
        "public",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_Edge_Current": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "edgebrowser",
        "-v",
        "13",
        "--vi",
        "public",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_IE_11": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "internetexplorer",
        "-v",
        "11",
        "--vi",
        "public",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    }
  },
  "launch_in_dev": [
    "PhantomJS"
  ],
  "launch_in_ci": [
    "SL_Chrome_Current",
    "SL_Firefox_Current",
    "SL_Safari_Current",
    "SL_Edge_Current",
    "SL_IE_11"
  ]
}
