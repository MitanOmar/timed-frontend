{
  "framework": "qunit",
  "test_page": "tests/index.html?hidepassed",
  "timeout": 600,
  "parallel": 5,
  "port": 7000,
  "browser_start_timeout": 90,
  "disable_watching": true,
  "launchers": {
    "SL_Firefox_Current": {
      "exe": "./node_modules/.bin/saucie",
      "args": [
        "-b",
        "firefox",
        "-v",
        "latest",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_Chrome_Current": {
      "exe": "./node_modules/.bin/saucie",
      "args": [
        "-b",
        "chrome",
        "-v",
        "latest",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_Safari_Current": {
      "exe": "./node_modules/.bin/saucie",
      "args": [
        "-b",
        "safari",
        "-v",
        "9",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_Safari_Last": {
      "exe": "./node_modules/.bin/saucie",
      "args": [
        "-b",
        "safari",
        "-v",
        "8",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_MS_Edge": {
      "exe": "./node_modules/.bin/saucie",
      "args": [
        "-b",
        "microsoftedge",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_IE_11": {
      "exe": "./node_modules/.bin/saucie",
      "args": [
        "-b",
        "internet explorer",
        "-v",
        "11",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_IE_10": {
      "exe": "./node_modules/.bin/saucie",
      "args": [
        "-b",
        "internet explorer",
        "-v",
        "10",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_IE_9": {
      "exe": "./node_modules/.bin/saucie",
      "args": [
        "-b",
        "internet explorer",
        "-v",
        "9",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    }
  },
  "launch_in_dev": [
    "PhantomJS"
  ],
  "launch_in_ci": [
    "SL_Chrome_Current",
    "SL_Firefox_Current",
    "SL_Safari_Current",
    "SL_Safari_Last",
    "SL_MS_Edge",
    "SL_IE_11",
    "SL_IE_10",
    "SL_IE_9"
  ]
}
